{
  "name": "bernankez",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "bun@1.1.0",
  "author": {
    "name": "Cole",
    "email": "bernankeic@gmail.com",
    "url": "https://github.com/Bernankez"
  },
  "license": "MIT",
  "homepage": "https://github.com/Bernankez/Bernankez#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/Bernankez.git"
  },
  "bugs": "https://github.com/Bernankez/Bernankez/issues",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepublishOnly": "nr build",
    "dev": "tsup --format esm,cjs --watch",
    "build": "tsup --format esm,cjs --clean",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . -f mo",
    "fix": "eslint . -f mo --fix"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "figlet": "^1.7.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@bernankez/eslint-config": "^1.1.0",
    "@types/figlet": "^1.5.8",
    "@types/node": "^20.12.5",
    "bumpp": "^9.4.0",
    "chalk": "^5.3.0",
    "eslint": "^9.0.0",
    "eslint-formatter-mo": "^1.2.0",
    "lint-staged": "^15.2.2",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4",
    "vite": "^5.2.8"
  },
  "simple-git-hooks": {
    "pre-commit": "nlx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
